	}

	.tribe-events-pro-week-grid__event--seq-6 {
		width: calc(50% - 2px);
	}

	.tribe-events-pro-week-grid__event--seq-7 {
		width: calc(40% - 2px);
	}

	.tribe-events-pro-week-grid__event--seq-8 {
		width: calc(30% - 2px);
	}

